{
  "name": "cate-desktop",
  "productName": "Cate Desktop",
  "version": "1.1.0-dev.1",
  "description": "A graphical user interface for the ESA CCI Toolbox (Cate)",
  "private": true,
  "scripts": {
    "start": "electron app",
    "test:e2e": "mocha \"e2e/**/*-test.js\"",
    "pretest": "npm run compile",
    "test": "mocha \"app/main/**/*.spec.js\" \"app/renderer/**/*.spec.js\" \"app/common/**/*.spec.js\"",
    "postinstall": "install-app-deps",
    "compile": "tsc",
    "compile:trace": "tsc --traceResolution",
    "dist": "npm run clean && npm run compile && build --x64 --publish never",
    "pack": "npm run clean && npm run compile && build --x64 --publish never --dir",
    "clean": "rimraf app/main app/renderer app/common npm-debug.log",
    "clean:dist": "rimraf dist",
    "clean:all": "npm run clean && rimraf dist app/node_modules && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CCI-Tools/cate-desktop.git"
  },
  "keywords": [
    "esa",
    "cci",
    "toolbox",
    "climate",
    "science"
  ],
  "author": "CCI Toolbox (Cate) Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CCI-Tools/cate-desktop/issues"
  },
  "homepage": "https://github.com/CCI-Tools/cate-desktop",
  "devDependencies": {
    "@blueprintjs/core": "^1.34.1",
    "@blueprintjs/datetime": "^1.25.0",
    "@blueprintjs/table": "^1.31.0",
    "@types/chai": "^3.4.35",
    "@types/chai-as-promised": "^0.0.31",
    "@types/chai-enzyme": "^0.5.7",
    "@types/classnames": "0.0.32",
    "@types/d3": "^4.7.0",
    "@types/deep-equal": "0.0.30",
    "@types/dom4": "^1.5.20",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/enzyme": "^2.5.29",
    "@types/geojson": "^1.0.2",
    "@types/mocha": "^2.2.41",
    "@types/oboe": "^2.0.28",
    "@types/openlayers": "^3.20.6",
    "@types/proj4": "^2.3.4",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-logger": "^3.0.5",
    "@types/reselect": "^2.0.27",
    "@types/tether": "^1.1.27",
    "cesium": "^1.40.0",
    "cesium-typings": "^1.40.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "classnames": "^2.2.5",
    "d3": "^3.5.17",
    "deep-equal": "^1.0.1",
    "electron": "1.7.9",
    "electron-builder": "19.48.3",
    "electron-devtools-installer": "^2.2.1",
    "enzyme": "^3.2.0",
    "mocha": "^4.0.1",
    "oboe": "^2.1.4",
    "openlayers": "^4.6.4",
    "proj4": "^2.4.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-transition-group": "^1.2.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.6.0",
    "semver": "^5.4.1",
    "spectron": "^3.4.1",
    "ts-node": "^4.0.1",
    "typescript": "^2.6.2"
  },
  "build": {
    "appId": "org.esa.ccitools.Cate",
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "app/resources/win32/cate-icon.ico",
      "publisherName": "Brockmann Consult GmbH"
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "app/resources/win32/cate-icon.ico"
    },
    "mac": {
      "icon": "app/resources/darwin/icon.icns",
      "category": "app.category.climate-science"
    },
    "dmg": {
      "icon": "app/resources/darwin/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Science"
    }
  }
}
