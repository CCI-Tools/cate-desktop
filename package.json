{
  "name": "cate-desktop",
  "productName": "Cate",
  "version": "0.0.1",
  "description": "ESA CCI Toolbox (Cate)",
  "private": true,
  "scripts": {
    "start": "electron app",
    "test:e2e": "mocha \"e2e/**/*-test.js\"",
    "pretest": "npm run compile",
    "test": "mocha \"app/main/**/*.spec.js\" \"app/renderer/**/*.spec.js\" \"app/common/**/*.spec.js\"",
    "postinstall": "install-app-deps",
    "compile": "tsc",
    "compile:trace": "tsc --traceResolution",
    "dist": "npm run clean && npm run compile && build --x64 --publish never",
    "pack": "npm run clean && npm run compile && build --x64 --publish never --dir",
    "clean": "rimraf app/main app/renderer app/common npm-debug.log",
    "clean:dist": "rimraf dist",
    "clean:all": "npm run clean && rimraf dist app/node_modules && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CCI-Tools/cate-desktop.git"
  },
  "keywords": [
    "esa",
    "cci",
    "toolbox",
    "climate",
    "science"
  ],
  "author": "Norman Fomferra, Marco ZÃ¼hlke (Brockmann Consult GmbH)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CCI-Tools/cate-desktop/issues"
  },
  "homepage": "https://github.com/CCI-Tools/cate-desktop",
  "devDependencies": {
    "@blueprintjs/core": "~1.13.x",
    "@blueprintjs/datetime": "~1.11.x",
    "@blueprintjs/table": "~1.10.x",
    "@types/chai": "~3.4.x",
    "@types/chai-as-promised": "~0.0.x",
    "@types/chai-enzyme": "~0.5.x",
    "@types/classnames": "0.0.x",
    "@types/deep-equal": "0.0.x",
    "@types/dom4": "~1.5.x",
    "@types/electron": "~1.4.x",
    "@types/electron-devtools-installer": "~2.0.x",
    "@types/enzyme": "~2.5.x",
    "@types/mocha": "~2.2.x",
    "@types/oboe": "~2.0.28",
    "@types/openlayers": "~3.20.x",
    "@types/proj4": "~2.3.x",
    "@types/react": "~0.14.x",
    "@types/react-addons-test-utils": "~0.14.x",
    "@types/react-dom": "~0.14.x",
    "@types/react-redux": "~4.4.x",
    "@types/redux-logger": "~2.6.x",
    "@types/reselect": "~2.0.x",
    "@types/tether": "~1.1.x",
    "cesium": "~1.30.x",
    "chai": "~3.5.x",
    "chai-as-promised": "~6.0.x",
    "chai-enzyme": "~0.6.x",
    "classnames": "~2.2.x",
    "deep-equal": "~1.0.x",
    "electron": "1.4.x",
    "electron-builder": "~15.4.x",
    "electron-devtools-installer": "~2.1.x",
    "enzyme": "~2.6.x",
    "mocha": "~3.2.x",
    "oboe": "~2.1.x",
    "openlayers": "~4.0.x",
    "proj4": "~2.4.x",
    "react": "~15.4.x",
    "react-addons-css-transition-group": "~15.4.x",
    "react-addons-perf": "~15.4.x",
    "react-addons-test-utils": "~15.4.x",
    "react-dom": "~15.4.x",
    "react-redux": "~5.0.x",
    "redux": "~3.6.x",
    "redux-logger": "~2.8.x",
    "redux-thunk": "~2.2.x",
    "reselect": "~2.5.x",
    "rimraf": "~2.5.x",
    "spectron": "~3.4.x",
    "ts-node": "~1.7.x",
    "typescript": "~2.1.x"
  },
  "build": {
    "appId": "org.esa.ccitools.Cate",
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "app/resources/win32/cate-icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "app/resources/win32/cate-icon.ico"
    },
    "mac": {
      "icon": "app/resources/darwin/icon.icns",
      "category": "app.category.climate-science"
    },
    "dmg": {
      "icon": "app/resources/darwin/icon.icns",
      "background": "app/resources/darwin/dmg-background.png"
    }
  }
}
