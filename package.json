{
  "name": "cate-desktop",
  "productName": "Cate Desktop",
  "version": "2.0.0-dev.15",
  "description": "A graphical user interface for the ESA CCI Toolbox (Cate)",
  "private": true,
  "scripts": {
    "start": "electron app",
    "test:e2e": "mocha \"e2e/**/*-test.js\"",
    "pretest": "npm run compile",
    "test": "mocha \"app/main/**/*.spec.js\" \"app/renderer/**/*.spec.js\" \"app/common/**/*.spec.js\"",
    "postinstall": "electron-builder install-app-deps",
    "compile": "tsc",
    "compile:trace": "tsc --traceResolution",
    "build": "build --x64 --publish never",
    "publish": "build --x64 --publish always",
    "publish:win": "build --x64 --win --publish always",
    "publish:mac": "build --x64 --mac --publish always",
    "dist": "npm run clean && npm run compile && npm run build",
    "pack": "npm run clean && npm run compile && build --x64 --publish never --dir",
    "clean": "rimraf app/main app/renderer app/common npm-debug.log",
    "clean:dist": "rimraf dist",
    "clean:all": "npm run clean && rimraf dist app/node_modules && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CCI-Tools/cate-desktop.git"
  },
  "keywords": [
    "esa",
    "cci",
    "toolbox",
    "climate",
    "science"
  ],
  "author": "CCI Toolbox (Cate) Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CCI-Tools/cate-desktop/issues"
  },
  "homepage": "https://github.com/CCI-Tools/cate-desktop",
  "devDependencies": {
    "@blueprintjs/core": "^1.35.5",
    "@blueprintjs/datetime": "^1.25.4",
    "@blueprintjs/table": "^1.31.2",
    "@types/chai": "^3.5.2",
    "@types/chai-as-promised": "^0.0.31",
    "@types/classnames": "0.0.32",
    "@types/d3-fetch": "^1.0.1",
    "@types/deep-equal": "0.0.30",
    "@types/dom4": "^1.5.20",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/geojson": "^1.0.6",
    "@types/mocha": "^2.2.41",
    "@types/node": "9.4.4",
    "@types/oboe": "^2.0.28",
    "@types/openlayers": "^3.20.6",
    "@types/proj4": "^2.3.4",
    "@types/react": "^16.0.40",
    "@types/react-color": "^2.13.4",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-logger": "^3.0.5",
    "@types/tether": "^1.1.27",
    "cesium": "^1.43.0",
    "cesium-typings": "^1.40.9",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "classnames": "^2.2.5",
    "d3-fetch": "^1.1.0",
    "deep-equal": "^1.0.1",
    "deep-object-diff": "^1.1.0",
    "electron": "1.8.4",
    "electron-builder": "19.56.0",
    "electron-devtools-installer": "2.2.3",
    "electron-log": "2.2.14",
    "electron-updater": "2.20.1",
    "jsdom": "11.6.2",
    "jsdom-global": "3.0.2",
    "mocha": "^5.0.4",
    "oboe": "^2.1.4",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-color": "^2.14.0",
    "react-dom": "^16.2.0",
    "react-linkify": "^0.2.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.2.0",
    "react-transition-group": "^1.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "spectron": "^3.8.0",
    "ts-node": "^4.1.0",
    "typescript": "2.8.3"
  },
  "build": {
    "appId": "org.esa.ccitools.Cate",
    "win": {
      "target": [
        "nsis"
      ],
      "publisherName": "Brockmann Consult GmbH"
    },
    "nsis": {
      "oneClick": true
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.education"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Science"
    }
  }
}
